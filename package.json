{
  "name": "nest-casl",
  "version": "0.0.1",
  "description": "Casl integration for NestJS",
  "author": "Denis Grankin",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bugs": {
    "url": "https://github.com/getjerry/nest-casl/issues"
  },
  "homepage": "https://github.com/getjerry/nest-casl#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getjerry/nest-casl.git"
  },
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint --cache .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "engines": {
    "node": ">= 12.0.0"
  },
  "dependencies": {
    "@casl/ability": "^5.3.1",
    "@ucast/core": "^1.10.0",
    "@ucast/mongo": "^2.4.1",
    "@ucast/sql": "^1.0.0-alpha.1"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@nestjs/graphql": "^7.6.15",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@nestjs/graphql": "^7.6.15",
    "@nestjs/platform-express": "^7.6.17",
    "@nestjs/testing": "^7.6.15",
    "@types/jest": "26.0.23",
    "@types/node": "^15.0.1",
    "@types/supertest": "^2.0.11",
    "apollo-server-express": "^2.24.1",
    "graphql": "^15.5.0",
    "jest": "^26.6.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.1.0",
    "supertest": "^6.1.3",
    "ts-jest": "26.5.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "testEnvironment": "node",
    "resetMocks": true
  }
}
